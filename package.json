{
  "name": "komunitin",
  "version": "0.0.1",
  "description": "Open System for Exchange Communities",
  "productName": "Komunitin",
  "author": "komunitin <komunitinbox@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env QENV=mirage quasar dev -m pwa",
    "dev-local": "cross-env QENV=local quasar dev -m pwa",
    "build-demo": "cross-env QENV=demo quasar build -m pwa",
    "build": "cross-env QENV=build quasar build -m pwa",
    "test": "quasar test --unit jest --dev=\"-m pwa\"",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --max-warnings 0 ./src",
    "start": "quasar serve --history dist/pwa",
    "test:coverage": "jest --silent --coverage src/",
    "test:watch": "jest --watch src/",
    "doc": "typedoc --out docs --tsconfig tsconfig.json src",
    "testAll": "tsc -p tsconfig.json --noEmit && npm run test && npm run lint"
  },
  "dependencies": {
    "@alexlit/vue-yandex-share": "^1.0.1",
    "@electron/typescript-definitions": "^8.8.0",
    "@quasar/extras": "^1.9.13",
    "@types/clamp-js": "^0.7.2",
    "@types/leaflet": "^1.5.21",
    "@types/marked": "^0.7.4",
    "@types/vuelidate": "^0.7.13",
    "axios": "^0.21.1",
    "clamp-js": "^0.7.0",
    "core-js": "^3.8.3",
    "date-fns": "^2.16.1",
    "firebase": "^9.6.0",
    "leaflet": "^1.7.1",
    "markdown-to-txt": "^1.0.1",
    "marked": "^0.8.2",
    "quasar": "^1.15.0",
    "vue-element-loading": "^1.1.6",
    "vue-i18n": "^8.27.0",
    "vue2-leaflet": "^2.6.0",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@quasar/app": "^2.1.14",
    "@quasar/quasar-app-extension-qenv": "^1.0.3",
    "@quasar/quasar-app-extension-testing": "^2.0.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.3",
    "@quasar/quasar-app-extension-typescript": "^1.0.2",
    "@types/faker": "^4.1.12",
    "@types/node": "^12.19.14",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/vue2-jest": "^26.0.1",
    "acorn": "^8.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "faker": "^4.1.0",
    "kind-of": ">=6.0.3",
    "minimist": ">=1.2.3",
    "miragejs": "^0.1.41",
    "node-sass": "^7.0.3",
    "typedoc": "^0.17.8",
    "typescript": "^4.5.5",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "defaults"
  ]
}
